#include<bits/stdc++.h>
using namespace std;


int main() {
    
    int n, d, ans = -1;
    cin>>n;
    
    int arr[n];
    for(int i=0; i<n; i++)
        cin>>arr[i];
    
    cin>>d;
     
    for(int i=0; i<n; i++)
    {
        if(arr[i] == d)
        {
            ans = i;
            break;
        }    
    }
    
    cout<<ans<<endl;
    return 0;
}
