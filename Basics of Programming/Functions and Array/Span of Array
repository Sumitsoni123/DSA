#include<bits/stdc++.h>
using namespace std;

int main() {
    
    int n, mn = INT_MAX, mx = INT_MIN;
    cin>>n;
    
    int arr[n];
    for(int i=0; i<n; i++)
    {
        cin>>arr[i];
        mn = min(mn, arr[i]);
        mx = max(mx, arr[i]);
    }
    
    cout<<(mx - mn)<<endl;
    
        
    return 0;    
}
