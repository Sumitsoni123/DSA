#include <bits/stdc++.h>
using namespace std;



int main() {
    
    int a,b,c;
    cin >> a >> b >> c;
    
    //write your code here
    int max = a;
    if (b >= max)
      max = b;

    if (c >= max)
      max = c;

    if (max == a && (b * b + c * c) == (a * a)) 
        cout<<"true"<<endl;
    else if (max == b && (a * a + c * c) == (b * b)) 
        cout<<"true"<<endl;
    else if(max == c && (a * a + b * b) == (c * c)) 
        cout<<"true"<<endl;
    
    else 
        cout<<"false"<<endl;
    
    return 0;
}

